{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/ncc",
      "config": {}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/.vercel/output/functions/render.func"
    }
  ],
  "output": {
    "sourceMap": true,  
    "includeFiles": ["dist/**"]
  }
}