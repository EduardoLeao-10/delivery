{
  "version": 2,
  "public": true,
  "rewrites": [
    { "source": "/_expo/static/(.*)", "destination": "/web-build/_expo/static/$1" },
    { "source": "/(.*)", "destination": "/web-build/index.html" }
  ],
  "builds": [
    {
      "src": "web-build/**/*",
      "use": "@vercel/static"
    }
  ]
}